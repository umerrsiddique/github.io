<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Country"
		class="sapUiPopupWithPadding"
		items="{f4help>/bnkcountry}"
		search="_handleValueHelpSearchCountry"
		confirm="_handleValueHelpCloseCountry"
		cancel="_handleValueHelpCloseCountry">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{f4help>landx50}"
			description="{f4help>land1}"/>
	</SelectDialog>
</core:FragmentDefinition>